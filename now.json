{
  "version": 2,
  "name": "hilo",
  "regions": ["cdg1"],
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": { "distDir": "build" }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1",
      "headers": { "cache-control": "public,max-age=31536000,immutable" }
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
